{
  "name": "@ticketplushq/arc-plugin-s3rver",
  "version": "0.0.0",
  "description": "Architect plugin for enable S3 on sandbox using s3rver",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "test:unit": "cross-env tape 'test/unit/**/*-test.js' | tap-spec",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "lint": "eslint . --fix",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticketplushq/arc-plugin-s3rver.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/package": "^7.2.0",
    "@architect/utils": "^3.0.4"
  },
  "devDependencies": {
    "@architect/eslint-config": "^2.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "humanize-number": "^0.0.2",
    "nyc": "^15.1.0",
    "s3rver": "^3.7.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.5.2"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config"
  }
}
